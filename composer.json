{
	"name": "simonmarcellinden/jwt",
	"description": "Efficient JWT auth for Laravel/Lumen. Easy integration, secure, lightweight.",
	"license": "MIT",
	"authors": [
		{
			"role": "Developer",
			"name": "Simon Marcel Linden",
			"email": "info@snerve.de",
			"homepage": "https://github.com/simonmarcellinden/"
		}
	],
	"homepage": "https://github.com/simonmarcellinden/jwt",
	"keywords": [
		"Laravel",
		"Lumen",
		"JWT",
		"Authentification"
	],
	"autoload": {
		"psr-4": {
			"SimonMarcelLinden\\JWT\\": "src/",
			"SimonMarcelLinden\\JWT\\Database\\": "database/",
			"SimonMarcelLinden\\JWT\\Database\\Factories\\": "src/database/factories/",
			"SimonMarcelLinden\\JWT\\Database\\Seeders\\": "src/database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Simonmarcellinden\\JWT\\Tests\\": "test"
		}
	},
	"require": {
		"firebase/php-jwt": "^6.10"
	},
	"extra": {
		"laravel": {
			"providers": [
				"SimonMarcelLinden\\JWT\\JWTServiceProvider"
			]
		}
	}
}